{
  "name": "rescript-pages",
  "version": "0.1.0",
  "jsx": { "version": 4, "mode": "automatic" },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "example",
      "subdirs": true,
      "type": "dev"
    },
    {
      "dir": "test",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".mjs",
  "bs-dependencies": [
    "@rescript/react",
    "rescript-asyncdata",
    "rescript-future",
    "rescript-request"
  ],
  "bs-dev-dependencies": ["rescript-test"],
  "warnings": {
    "error": "+101"
  }
}
