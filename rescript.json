{
  "name": "rescript-pages",
  "version": "0.1.0",
  "jsx": { "version": 4 },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "example",
      "subdirs": true,
      "type": "dev"
    },
    {
      "dir": "test",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".mjs",
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "rescript-asyncdata",
    "rescript-future",
    "rescript-request"
  ],
  "bs-dev-dependencies": ["rescript-nodejs", "rescript-test"],
  "warnings": {
    "error": "+101"
  },
  "bsc-flags": ["-open RescriptCore"]
}
