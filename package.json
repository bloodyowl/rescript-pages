{
  "name": "rescript-pages",
  "version": "4.0.0",
  "scripts": {
    "clean": "rescript clean",
    "build": "rescript build -with-deps",
    "watch": "rescript build -with-deps -w -ws 9999",
    "build-website": "node bin/pages.mjs build example/App.mjs",
    "test": "node bin/pages.mjs build example/App.mjs && retest test/*.mjs",
    "server": "node bin/pages.mjs start example/App.mjs 3000",
    "test-ci": "retest test/*.mjs"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "module",
  "bin": {
    "pages": "bin/pages.mjs"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "bloodyowl <bloodyowl@icloud.com>",
  "license": "MIT",
  "devDependencies": {
    "@rescript/react": "^0.12.0",
    "rescript": "^11.0.0",
    "rescript-nodejs": "^16.1.0",
    "rescript-test": "^7.0.0"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/server": "^11.0.0",
    "@rescript/core": "^1.3.0",
    "bs-fetch": "^0.6.2",
    "chalk": "4.1.1",
    "chokidar": "^3.4.3",
    "copy-webpack-plugin": "^6.3.0",
    "express": "^4.17.1",
    "front-matter": "^4.0.2",
    "get-port": "^5.1.1",
    "gh-pages": "^3.1.0",
    "highlight.js": "^10.4.1",
    "html-webpack-plugin": "^5.2.0",
    "inline-translations-webpack-plugin": "^1.0.2",
    "memfs": "^3.2.0",
    "mime": "^2.4.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "reason-highlightjs": "^0.2.1",
    "remarkable": "^2.0.1",
    "rescript-asyncdata": "^4.0.0",
    "rescript-devserver-tools": "^1.0.16",
    "rescript-future": "^2.1.0",
    "rescript-request": "^3.0.1",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^4.5.0",
    "ws": "^7.4.0"
  },
  "peerDependencies": {
    "@emotion/css": "^11.1.3",
    "@rescript/react": "^0.12.0",
    "react": "^18.0.1",
